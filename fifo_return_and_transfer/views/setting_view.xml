<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="res_config_settings_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.form</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="stock.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <div id="production_lot_info" position="after">
                    <h2>Internal Transfer With Cost</h2>
                    <div class="row mt16 o_settings_container">
                        <div class="row col-xs-12 col-md-12 o_setting_box">
                            <div class="col-xs-4 col-md-4">
                                <label for="return_picking_type_id"/>

                            </div>
                            <div class="col-xs-8 col-md-8">
                                    <field name="return_picking_type_id"/>
                            </div>
                        </div>
                        <div class=" row col-xs-12 col-md-12 o_setting_box">
                            <div class="col-xs-4 col-md-4">
                                <label for="internal_receipt_id"/>

                            </div>
                            <div class="col-xs-8 col-md-8">
                                    <field name="internal_receipt_id"/>
                            </div>
                        </div>
                    </div>
                </div>
        </field>
    </record>
</odoo>